// Binary Operators
inline Vector3D<T> operator$${OP}$$(const Vector2D<T>& B)
{
	return Vector3D<T>(
		(this->x() $${OP}$$ B.x()),
		(this->y() $${OP}$$ B.y()),
		(this->z() $${OP}$$ B.z())
	);
};
inline Vector3D<T> operator$${OP}$$(const T& B)
{
	return Vector3D<T>(
		(this->x() $${OP}$$ B),
		(this->y() $${OP}$$ B),
		(this->z() $${OP}$$ B)
	);
};

// Binary Assignment Operators
inline Vector3D<T>& operator$${OP}$$=(const Vector2D<T>& B)
{
	this->x() = this->x() $${OP}$$ B.x();
	this->y() = this->y() $${OP}$$ B.y();
	this->z() = this->z() $${OP}$$ B.z();
	return (*this);
};
inline Vector3D<T>& operator$${OP}$$=(const T& B)
{
	this->x() = this->x() $${OP}$$ B;
	this->y() = this->y() $${OP}$$ B;
	this->z() = this->z() $${OP}$$ B;
	return (*this);
};