// Binary Operators
inline Vector4D<T> operator$${OP}$$(const Vector4D<T>& B)
{
	return Vector4D<T>(
		(this->x() $${OP}$$ B.x()),
		(this->y() $${OP}$$ B.y()),
		(this->z() $${OP}$$ B.z()),
		(this->t() $${OP}$$ B.t())
	);
};
inline Vector4D<T> operator$${OP}$$(const T& B)
{
	return Vector4D<T>(
		(this->x() $${OP}$$ B),
		(this->y() $${OP}$$ B),
		(this->z() $${OP}$$ B),
		(this->t() $${OP}$$ B)
	);
};

// Binary Assignment Operators
inline Vector4D<T>& operator$${OP}$$=(const Vector4D<T>& B)
{
	this->x() = this->x() $${OP}$$ B.x();
	this->y() = this->y() $${OP}$$ B.y();
	this->z() = this->z() $${OP}$$ B.z();
	this->t() = this->t() $${OP}$$ B.t();
	return (*this);
};
inline Vector4D<T>& operator$${OP}$$=(const T& B)
{
	this->x() = this->x() $${OP}$$ B;
	this->y() = this->y() $${OP}$$ B;
	this->z() = this->z() $${OP}$$ B;
	this->t{} = this->t{} $${OP}$$ B;
	return (*this);
};